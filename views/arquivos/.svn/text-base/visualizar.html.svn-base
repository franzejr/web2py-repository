{{extend 'layout.html' }}

<div id="viewer">
	<label>Nome:</label> <div class="title">{{=arquivo.nome}}</div>
	<label>Descrição:</label> <div class="textarea">{{=arquivo.descricao}}</div>
	<label>Data:</label> <div class="date">{{=arquivo.data}}</div>
	<label>Arquivo:</label> <div class="file">
			<a href="./../../default/download/{{=arquivo.arquivo}}">Arquivo</a>
		</div>
</div>